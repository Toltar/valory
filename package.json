{
	"name": "valory",
	"version": "3.5.1",
	"description": "A server agnostic web framework for creating bulletproof apis",
	"typings": "dist/main.d.ts",
	"main": "dist/main.js",
	"scripts": {
		"pretest": "(rm src/test/generatedRoutes.ts || true) && npm run build",
		"test": "mocha dist/test/test.js",
		"build": "tsc && npm run types && npm run runtime",
		"types": "tsc -p tsconfig-defs.json",
		"release": "semantic-release",
		"runtime": "cp package-min.json pkg/package.json",
		"lint": "tslint --project .",
		"clean": "rm -r dist pkg || true",
		"docgen": "typedoc --module commonjs --excludePrivate --out docs --excludeNotExported --ignoreCompilerErrors --mode file src/server"
	},
	"engines": {
		"node": ">=8"
	},
	"author": "John Conley",
	"license": "ISC",
	"files": [
		"package.json",
		"README.md",
		"dist",
		"templates",
		"tsconfig.json",
		"tslint.json",
		"tsfmt.json"
	],
	"devDependencies": {
		"@commitlint/cli": "^7.2.1",
		"@commitlint/config-conventional": "^7.1.2",
		"@types/bluebird-global": "^3.5.9",
		"@types/chai": "^4.1.6",
		"@types/inquirer": "^0.0.43",
		"@types/json-schema": "^7.0.1",
		"@types/lodash": "^4.14.117",
		"@types/minimatch": "^3.0.3",
		"@types/node": "^10.12.0",
		"@types/pino": "^5.20.0",
		"@types/request": "^2.47.1",
		"@types/request-promise": "^4.1.42",
		"@types/semver": "^5.5.0",
		"@types/steed": "^1.1.2",
		"@types/swagger-parser": "^4.0.2",
		"@types/yargs": "^12.0.1",
		"babel-core": "^6.26.3",
		"babel-plugin-loop-optimizer": "^1.4.1",
		"babel-plugin-transform-async-to-bluebird": "^1.1.1",
		"chai": "^4.2.0",
		"cz-conventional-changelog": "^2.1.0",
		"fast-async": "^6.3.8",
		"husky": "^1.1.2",
		"mocha": "5.0.5",
		"mocha-typescript": "^1.1.17",
		"request": "^2.88.0",
		"request-promise": "^4.2.2",
		"semantic-release": "^15.10.3",
		"typedoc": "^0.13.0"
	},
	"dependencies": {
		"ajv": "^6.5.4",
		"bluebird": "^3.5.2",
		"chalk": "^2.4.1",
		"dot": "^1.1.2",
		"fastify": "^1.12.1",
		"flatstr": "^1.0.8",
		"google-closure-compiler": "^20181008.0.0",
		"hyperid": "^1.4.1",
		"inquirer": "^6.2.0",
		"json-schema-merge-allof": "^0.6.0",
		"lodash": "^4.17.11",
		"lodash-deep": "^2.0.0",
		"map-keys-deep-lodash": "^1.2.1",
		"minimatch": "^3.0.4",
		"moment": "^2.22.2",
		"pino": "^5.8.0",
		"semver": "^5.6.0",
		"steed": "^1.1.3",
		"swagger-parser": "^6.0.1",
		"thread-spin": "^1.2.1",
		"tmp": "^0.0.33",
		"ts-node": "^7.0.1",
		"tstl": "^2.0.7",
		"typescript": "^3.1.3",
		"xxhashjs": "^0.2.2",
		"yargs": "^12.0.2"
	},
	"preferGlobal": true,
	"bin": {
		"valory": "dist/lib/cli.js"
	},
	"repository": "https://github.com/jfrconley/valory.git",
	"release": {
		"repositoryUrl": "https://github.com/jfrconley/valory.git",
		"branch": "master",
		"verifyConditions": [
			{
				"path": "@semantic-release/github"
			},
			{
				"path": "@semantic-release/npm"
			},
			{
				"path": "@semantic-release/npm",
				"pkgRoot": "pkg"
			}
		],
		"publish": [
			{
				"path": "@semantic-release/npm"
			},
			{
				"path": "@semantic-release/github"
			},
			{
				"path": "@semantic-release/npm",
				"pkgRoot": "pkg"
			}
		],
		"prepare": [
			{
				"path": "@semantic-release/npm"
			},
			{
				"path": "@semantic-release/npm",
				"pkgRoot": "pkg"
			}
		],
		"success": [],
		"fail": []
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"keywords": [
		"rest",
		"framework",
		"validation",
		"decorators"
	],
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
		}
	}
}
