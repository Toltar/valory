{
	"name": "valory",
	"version": "3.5.1",
	"description": "A server agnostic web framework for creating bulletproof apis",
	"scripts": {
		"pretest": "(rm src/test/generatedRoutes.ts || true) && npm run buildTest",
		"test": "mocha dist/test/test.js",
		"buildTest": "tsc && npm run build",
		"build": "npm run compiler && npm run runtime",
		"compiler": "tsc -p tsconfig-compiler.json",
		"release": "semantic-release",
		"runtime": "tsc -p tsconfig-runtime.json && cp package-min.json runtime/package.json",
		"lint": "tslint --project .",
		"clean": "rm -r dist compiler runtime || true",
		"docgen": "typedoc --module commonjs --target ES6 --includes DOCS.md --excludeExternals --excludePrivate --out docs --excludeNotExported --ignoreCompilerErrors --mode file  src/server/"
	},
	"engines": {
		"node": ">=8"
	},
	"author": "John Conley",
	"license": "ISC",
	"files": [
		"package.json",
		"README.md",
		"compiler",
		"templates",
		"tsconfig.json",
		"tslint.json",
		"tsfmt.json"
	],
	"devDependencies": {
		"@commitlint/cli": "^7.4.0",
		"@commitlint/config-conventional": "^7.3.1",
		"@types/bluebird-global": "^3.5.9",
		"@types/chai": "^4.1.7",
		"@types/inquirer": "^0.0.43",
		"@types/json-schema": "^7.0.1",
		"@types/lodash": "^4.14.120",
		"@types/minimatch": "^3.0.3",
		"@types/node": "^10.12.19",
		"@types/pino": "^5.20.0",
		"@types/request": "^2.48.1",
		"@types/request-promise": "^4.1.42",
		"@types/semver": "^5.5.0",
		"@types/steed": "^1.1.2",
		"@types/swagger-parser": "^4.0.2",
		"@types/yargs": "^12.0.8",
		"babel-core": "^6.26.3",
		"babel-plugin-loop-optimizer": "^1.4.1",
		"babel-plugin-transform-async-to-bluebird": "^1.1.1",
		"chai": "^4.2.0",
		"cz-conventional-changelog": "^2.1.0",
		"fast-async": "^6.3.8",
		"husky": "^1.3.1",
		"mocha": "5.2.0",
		"mocha-typescript": "^1.1.17",
		"request": "^2.88.0",
		"request-promise": "^4.2.2",
		"semantic-release": "^15.13.3",
		"tslint": "^5.12.1",
		"typedoc": "^0.14.2"
	},
	"dependencies": {
		"ajv": "^6.7.0",
		"bluebird": "^3.5.3",
		"chalk": "^2.4.2",
		"dot": "^1.1.2",
		"fastify": "^1.13.4",
		"flatstr": "^1.0.9",
		"google-closure-compiler": "^20190215.0.1",
		"hyperid": "^2.0.2",
		"inquirer": "^6.2.1",
		"json-schema-merge-allof": "^0.6.0",
		"lodash": "^4.17.11",
		"lodash-deep": "^2.0.0",
		"map-keys-deep-lodash": "^1.2.2",
		"minimatch": "^3.0.4",
		"moment": "^2.24.0",
		"pino": "^5.11.1",
		"semver": "^5.6.0",
		"steed": "^1.1.3",
		"swagger-parser": "^6.0.5",
		"thread-spin": "^1.2.1",
		"tmp": "^0.0.33",
		"ts-node": "^8.0.2",
		"tstl": "^2.1.1",
		"typescript": "^3.2.4",
		"xxhashjs": "^0.2.2",
		"yargs": "^12.0.5"
	},
	"preferGlobal": true,
	"bin": {
		"valory": "compiler/lib/cli.js"
	},
	"repository": "https://github.com/valoryteam/valory.git",
	"release": {
		"repositoryUrl": "https://github.com/valoryteam/valory.git",
		"branch": "master",
		"verifyConditions": [
			{
				"path": "@semantic-release/github"
			},
			{
				"path": "@semantic-release/npm"
			},
			{
				"path": "@semantic-release/npm",
				"pkgRoot": "runtime"
			}
		],
		"publish": [
			{
				"path": "@semantic-release/npm"
			},
			{
				"path": "@semantic-release/github"
			},
			{
				"path": "@semantic-release/npm",
				"pkgRoot": "runtime"
			}
		],
		"prepare": [
			{
				"path": "@semantic-release/npm"
			},
			{
				"path": "@semantic-release/npm",
				"pkgRoot": "runtime"
			}
		],
		"success": [],
		"fail": []
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org"
	},
	"keywords": [
		"rest",
		"framework",
		"validation",
		"decorators"
	],
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
		}
	}
}
